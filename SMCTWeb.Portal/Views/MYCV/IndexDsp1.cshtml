<script>
    async function exportPDF() {
        $("#dvToolbar").css('display', 'none');
      
        const { jsPDF } = window.jspdf;

        const element = document.getElementById('dvContent');

        // Use html2canvas to convert the element to canvas
        const canvas = await html2canvas(element);
        const imgData = canvas.toDataURL('image/png');

        // Create a new jsPDF instance
        const pdf = new jsPDF('p', 'mm', 'a4');

        // Add the image to the PDF
        const imgProps = pdf.getImageProperties(imgData);
        const pdfWidth = pdf.internal.pageSize.getWidth();
        const pdfHeight = (imgProps.height * pdfWidth) / imgProps.width;
        pdf.addImage(imgData, 'PNG', 0, 0, pdfWidth, pdfHeight);
        pdf.setGState(new pdf.GState({ opacity: 0.3 }));

        // Add image watermark
        const watermarkImg = 'data:image/png;base64,'; // Your watermark image in base64
        const watermarkWidth = pdfWidth / 2;
        const watermarkHeight = watermarkWidth * (imgProps.height / imgProps.width);
      //  pdf.addImage(watermarkImg, 'PNG', pdfWidth / 4, pdfHeight / 4, watermarkWidth, watermarkHeight, '', 'FAST');
        pdf.addImage(watermarkImg, 'PNG', 160, -10, 50, 50, '', 'FAST');


        // Add watermark with reduced opacity
        pdf.setFontSize(40);
        pdf.setTextColor(150, 150, 150);

        // Save the graphics state
        pdf.saveGraphicsState();
        // Set the opacity (alpha) to 0.3
        pdf.setGState(new pdf.GState({ opacity: 0.5 }));
        // Add the watermark text
        pdf.text('Verify by Surat Thani Gov', pdfWidth , pdfHeight / 2, {
            angle: 45,
            align: 'center'
        });
        // Save the PDF
        pdf.save('Resume.pdf');
        onprocess();
       
    }
</script>
<script>
    function onprocess() {
        let timerInterval;
        Swal.fire({
            title: "Please wait..!",
            html: "Resume is loading.. <b></b> milliseconds.",
            timer: 1000,
            timerProgressBar: true,
            didOpen: () => {
                Swal.showLoading();
                const timer = Swal.getPopup().querySelector("b");
                timerInterval = setInterval(() => {
                    timer.textContent = `${Swal.getTimerLeft()}`;
                }, 100);
            },
            willClose: () => {
                clearInterval(timerInterval);
            }
        }).then((result) => {
            /* Read more about handling dismissals below */
            if (result.dismiss === Swal.DismissReason.timer) {
                console.log("resume is completed");
            }
        });
        $("#dvToolbar").css('display', 'inline-table');
    }
</script>
