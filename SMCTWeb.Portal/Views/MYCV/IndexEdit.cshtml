                    </ol>
                    <span class="cd-h-timeline__filling-line" aria-hidden="true" style="transform: scaleX(0.0652874);"></span>
                </div>
            </div>
            <ul>
                <li><a href="#0" class="text-replace cd-h-timeline__navigation cd-h-timeline__navigation--prev cd-h-timeline__navigation--inactive">Prev</a></li>
                <li><a href="#0" class="text-replace cd-h-timeline__navigation cd-h-timeline__navigation--next">Next</a></li>
            </ul>
        </div>
        <div class="cd-h-timeline__events" style="height:1000px">
            <ol>
               
                <li class="cd-h-timeline__event text-component">
                    <div class="cd-h-timeline__event-content container">
                  
                        <!-- component -->
                        <div class="p-1">
                            <div class="xcontainer">

                                <div class="card-body">
                                    <form action="@Url.Action("editCV")">
                                        
                                        <div class="form-row">

                                            <div class="col-md-6">
                                                <label>First Name</label>
                                                <input type="text" asp-for="Name" placeholder="First Name" class="form-control" />
                                            </div>
                                            <div class="col-md-6">
                                                <label>Last Name</label>
                                                <input type="text" asp-for="SureName" placeholder="Last Name" class="form-control" />
                                            </div>
                                        </div>
                                        <div class="form-row">
                                            <div class="col-md-6">
                                                <label>Date of Birth</label>
                                                <input type="date" asp-for="DateOfBirth" class="form-control" />
                                            </div>
                                            <div class="col-md-6">
                                                <label>Address</label>
                                                <input type="text" asp-for="Address" placeholder="Last Name" class="form-control" />

                                            </div>
                                        </div>
                                        <div class="form-row">
                                            <div class="col-md-6">
                                                <label>Email Address</label>
                                                <input type="email" asp-for="Email" class="form-control" placeholder="Enter Email Address" />
                                            </div>
                                            <div class="col-md-6">
                                                <label>Mobile Number</label>
                                                <input type="tel" asp-for="PhoneNo" pattern="^\d{10}$" placeholder="Enter Mobile Number" class="form-control" />
                                            </div>
                                        </div>
                                        <div class="form-row">
                                            <div class="col-md-12">
                                                <label>HashTag,ตำแหน่งงานที่สนใจ</label>

                                                <input type="tel" asp-for="HashTag" placeholder="HashTag" class="form-control" />

                                            </div>

                                        </div>
                                        <div class="form-row">
                                            <div class="col-md-12">
                                                <label>คำอธิบายตัวคุณ</label>

                                                <textarea placeholder="Descripe yourself" rows="1" asp-for="Description" class="form-control">

                                               </textarea>
                                            </div>

                                        </div>
                                        <div class="form-row">
                                            <div class="col-md-12">
                                                <label>รายละเอียดเพิ่มเติมเกี่ยวกับตัวคุณ</label>
                                               
                                                <textarea placeholder="Descripe yourself" rows="5" asp-for="FullDescription" class="form-control">

                                               </textarea>
                                            </div>
                                             
                                        </div>
                                        <div class="form-row">

                                            <div class="col-md-6">
                                                <label>Profile Picture</label>
                                              @*   <input type="file" asp-for="Avatar" class="form-control" /> *@
                                            </div>
                                        </div>
                                        <div style="margin-top:15px;">

                                            <button type="submit" onclick="return isCanEditCV()" class="btn btn-primary rounded-1">บันทึก</button>

                                            <a href="@Url.Action("index","MyCV")" class="btn btn-warning rounded-1">กลับ</a>
                                           <script>
                                               function isCanEditCV() {
                                                   var err = '';
                                                  
                                                   if (document.getElementById("FullDescription").value.length > 500) { err = 'รายละเอียดเพิ่มเติมเกินความยาวที่กำหนด 500 ตัวอักษร';}
                                                   if (err == '') { return true; } else {
                                                       swal.fire(err);
                                                       return false;
                                                   }
                                               }
                                           </script>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                        <!-- /.container -->
                            <div class="mt-8 p-4" style="display:none">
                                 
