    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script>



        if ("@ViewBag.Msg.text" != "") { showMsg(); }
        function showMsg() {

            Swal.fire({
                title: "@ViewBag.Msg.title",
                text: "@ViewBag.Msg.text",
                icon: "@ViewBag.Msg.icon"
            });
        }
    </script>
  </head>
  <body>
